    "import numpy as np\n",
    "import matplotlib.pyplot as plt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "f1bc073a-ac7d-469b-a2a0-d99df5dcc152",
   "metadata": {},
   "outputs": [],
   "source": [
    "def gau(x):\n",
    "    sigma = 1.0\n",
    "    return 1.0/(np.sqrt(2*np.pi*sigma**2))*np.exp(-x**2/(2*sigma**2))"
   ]
  },
